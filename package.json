{
  "name": "nextjs-fullstack-app-template",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=14.0.0",
    "yarn": ">=1.22.0",
    "npm": "please use yarn"
  },
  "scripts": {
    "dev": "next dev --turbopack",
